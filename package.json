{
  "name": "budget",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pg": "^8.18.0",
    "plaid": "^31.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-plaid-link": "^3.3.2"
  },
  "scripts": {
    "start": "bun run build && bun run start-server",
    "start-server": "bun ./build/server/bundle.js",
    "build": "bun run build-server && bun run build-client",
    "build-client": "vite build",
    "build-server": "bun ./src/server/pack.ts",
    "dev": "bun run dev-client & bun run dev-server",
    "dev-client": "vite",
    "dev-server": "bun --watch ./src/server/start.ts",
    "test": "bun test",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bun": "^1.1.0",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.17.4",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
