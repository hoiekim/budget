{
  "name": "budget",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@elastic/elasticsearch": "^8.7.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "plaid": "^31.0.0"
  },
  "scripts": {
    "start": "npm run build && npm run start-server",
    "start-server": "node ./build/server/bundle.js",
    "build": "npm run build-server && npm run build-client",
    "build-client": "react-scripts build",
    "build-server": "npm run ts-compile-server && npm run webpack-server",
    "webpack-server": "node ./build/server/server/pack.js",
    "ts-compile-server": "tsc -p ./tsconfig.server.json",
    "dev-client": "react-scripts start",
    "dev-server": "nodemon ./src/server/start.ts",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/webpack-node-externals": "^2.5.3",
    "nodemon": "^3.1.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-plaid-link": "^3.3.2",
    "react-scripts": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-node-externals": "^3.0.0"
  },
  "proxy": "http://localhost:3005"
}
